    },
    {
      "atom_id": "A070",
      "start_ms": 1525000,
      "end_ms": 1532000,
      "duration_ms": 7000,
      "start_time": "00:25:25",
      "end_time": "00:25:32",
      "duration_seconds": 7.0,
      "merged_text": "国民党残兵4,000人，段锡文将军带去泰国美斯乐了，但是呢这些人里边不是一条心",
      "type": "叙述历史",

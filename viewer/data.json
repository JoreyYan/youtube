      ]
    },
    {
      "atom_id": "A075",
      "start_ms": 1640000,
      "end_ms": 1649000,
      "duration_ms": 9000,
      "start_time": "00:27:20",
      "end_time": "00:27:29",
      "duration_seconds": 9.0,
      "merged_text": "昆沙军队参谋长，总参谋长是张苏权将军啊，这所有的东西都昆沙甩手掌柜的，不管都交给张苏权",
      "type": "叙述历史",
      "completeness": "完整",
